chmod 400 WebServer-1-Key.pem

ssh -i WebServer-1-Key.pem ubuntu@<IP>

sudo apt update && sudo apt upgrade -y
sudo apt install nodejs npm mysql-client -y

mkdir student-records-app && cd student-records-app
npm init -y
npm install express mysql2 bcryptjs express-session cookie-parser

nano db.js
nano index.js
nano package.json

mkdir public && cd public

nano add-student.html
nano admin-passwords.html
nano edit-student.html
nano index.html
nano login.html
nano my-record.html
nano signup.html
nano students-list.html
